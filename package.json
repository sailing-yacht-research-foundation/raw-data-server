{
  "name": "raw-data-server",
  "version": "0.3.10",
  "description": "Server that will be used to transform data to parquet format and perform bulk saving into Permanent Storage",
  "main": "src/main.js",
  "scripts": {
    "start": "node src/main.js",
    "dev": "nodemon -L src/main.js",
    "lint": "eslint --max-warnings 0 \"src/**/*.js\"",
    "test": "jest --coverage --runInBand --detectOpenHandles --forceExit",
    "db:migrate": "node scripts/migrate",
    "db:seed": "sequelize db:seed:all",
    "db:syrf:sync": "node src/syrf-schema/sync",
    "db:syrf:migrate": "node src/syrf-schema/migrate",
    "debug": "nodemon --inspect -L src/main.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@syrf/circuit-breaker": "^1.0.1",
    "@syrf/transport-library": "^1.0.7",
    "@turf/turf": "^6.5.0",
    "JSONStream": "^1.3.5",
    "all-the-cities": "^3.1.0",
    "aws-sdk": "^2.903.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "bson": "4.5.1",
    "csvtojson": "^2.0.10",
    "date-fns": "^2.25.0",
    "date-fns-tz": "^1.1.6",
    "dotenv": "^8.5.1",
    "express": "^4.17.1",
    "express-status-monitor": "^1.3.3",
    "extract-zip": "^2.0.1",
    "geokdbush": "^1.1.0",
    "ical-generator": "^3.0.1",
    "jimp": "^0.16.1",
    "jsonfile": "^6.1.0",
    "kdbush": "^3.0.0",
    "moment": "^2.29.1",
    "multer": "^1.4.2",
    "parquetjs-lite": "^0.8.7",
    "pg": "^8.7.1",
    "puppeteer": "^10.4.0",
    "qs": "^6.10.1",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.2.0",
    "sharp": "^0.29.3",
    "tar-fs": "2.1.1",
    "temp": "^0.9.4",
    "umzug": "^2.3.0",
    "uuid": "^8.3.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.2.5",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3"
  }
}
